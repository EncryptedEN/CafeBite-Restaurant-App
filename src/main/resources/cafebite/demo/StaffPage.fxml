<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.text.Font?>
<VBox stylesheets="@styles.css" alignment="CENTER" prefHeight="368.0" prefWidth="354.0" spacing="20.0" xmlns="http://javafx.com/javafx/21"
      xmlns:fx="http://javafx.com/fxml/1" fx:controller="cafebite.demo.StaffPageController">
    <Label text="Primary View" />
    <Button fx:id="loginButton" mnemonicParsing="false" onAction="#switchToLogin" text="Login" />
    <Button text="Back" onAction="#handleBack" styleClass="back-button">
        <font>
            <Font size="14"/>
        </font>
    </Button>
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
</VBox>
